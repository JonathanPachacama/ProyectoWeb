<h1>Usted esta editando a:
  <%=  Hoteles.nombreHotel %>
</h1>

<br>
<br>
<br>

<form action="Hotel/editarHoteles" method="post">


  <input hidden
         class="form-control"
         type="text"
         id="id"
         name="id"
         value="<%= Hoteles.id %>">


  <div class="form-group row">
    <label for="nombreHotel" class="col-2 col-form-label">Nombre Hotel</label>
    <div class="col-10">
      <input class="form-control"
             type="text"
             placeholder="Ingrese nombre del hotel"
             id="nombreHotel"
             name="nombreHotel"
             value="<%= Hoteles.nombreHotel %>"

      >
    </div>
  </div>

  <div class="form-group row">
    <label for="direccionHotel" class="col-2 col-form-label">Direccion del Hotel</label>
    <div class="col-10">
      <input class="form-control" type="text"
             placeholder="Ingrese direccion"
             name="direccionHotel"
             id="direccionHotel"
             value="<%= Hoteles.direccionHotel %>"
      >
    </div>
  </div>

  <div class="form-group row">
    <label for="precioEstadia" class="col-2 col-form-label">Precion Estadia</label>
    <div class="col-10">
      <input class="form-control" type="text" placeholder="Ingrese el precio de la estadia" id="precioEstadia"
             name="precioEstadia"
             value="<%= Hoteles.precioEstadia %>">
    </div>
  </div>
  <br>
  <br>

  <button
    class="btn btn-outline-success">Editar</button>

</form>
